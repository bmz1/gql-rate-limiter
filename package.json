{
  "name": "graphql-distributed-rate-limiter",
  "version": "1.0.0",
  "description": "A distributed leaky bucket rate limiter using Redis and TypeScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --testTimeout 15000",
    "test:watch": "vitest --testTimeout 15000 --watch",
    "format": "prettier --write \"**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmz1/distributed-rate-limiter.git"
  },
  "keywords": [
    "rate-limiter",
    "graphql",
    "redis",
    "typescript",
    "leaky-bucket",
    "shopify"
  ],
  "author": "BMZ",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  },
  "dependencies": {
    "ioredis": "^5.4.2"
  }
}
